{
  "author": "Matheus Lucas Batista",
  "license": "MIT",
  "name": "projeto-novo",
  "version": "1.0.0",
  "description": "Serviço para gerenciamento de usuários",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon server.js",
    "seed": "node src/seeds/seeds.js",
    "start": "node server.js",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-fileupload": "^1.5.2",
    "faker-br": "^0.4.1",
    "fs": "^0.0.1-security",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.15.0",
    "mongoose": "^8.13.2",
    "mongoose-paginate-v2": "^1.9.0",
    "mongoose-schema-jsonschema": "^3.0.2",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.5",
    "path": "^0.12.7",
    "sharp": "^0.34.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "url": "^0.11.4",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/helper-annotate-as-pure": "^7.27.3",
    "@babel/preset-env": "^7.27.2",
    "@eslint/js": "^9.26.0",
    "@jest/globals": "^30.0.0-beta.3",
    "babel-jest": "^29.7.0",
    "babel-plugin-transform-import-meta": "^2.3.2",
    "eslint": "^9.26.0",
    "eslint-plugin-jest": "^28.12.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.2",
    "nodemon": "^3.1.9",
    "supertest": "^7.1.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "babel-plugin-transform-import-meta"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testEnvironment": "node",
    "verbose": true,
    "transformIgnorePatterns": [
      "/node_modules/(?!node-fetch)/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/utils/helpers/index.js",
      "/src/config/dbConnect.js",
      "/src/controllers/AuthController.js",
      "/src/service/AuthService.js",
      "/utils/logger.js",
      "/utils/errors",
      "/middlewares/",
      "/utils/AuthHelper.js",
      "/utils/TokenUtil.js",
      "src/models/Rota.js",
      "/utils/helpers/errorHandler.js",
      "/utils/helpers/StatusService.js",
      "/utils/helpers/messages.js",
      "/utils/helpers/CommonResponse.js",
      "/middlewares/asyncWrapper.js",
      "utils/helpers/CustomError.js",
      "utils/helpers/HttpStatusCodes.js",
      "utils/validators/schemas/zod/querys/GrupoQuerySchema.js",
      "utils/validators/schemas/zod/querys/RequestAuthorizationSchema.js",
      "utils/validators/schemas/zod/GrupoSchema.js",
      "utils/validators/schemas/zod/LoginSchema.js",
      "src/service/PermissionService.js",
      "src/repository/filters/DemandaFilterBuild.js"
    ]
  }
}
